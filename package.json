    "android-release-apk": "cd src/7_shared/scripts/android && sh buildApk.sh && cd ../../../../ && open ./android/app/build/outputs/apk/",
    "android-release-aab": "cd src/7_shared/scripts/android && sh buildAab.sh && cd ../../../../ && open ./android/app/build/outputs/bundle/release",
